{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "index.js"
        }
      ],
    "rewrites": [{ "source": "/(.*)", "destination": "index.js" }]
  }